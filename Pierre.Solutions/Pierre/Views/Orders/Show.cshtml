@using Pierre.Models;
@{
  Layout = "_Layout";
}

<h1>@Model["order"].Title</h1>
<p>Description: @Model["order"].Description</p>
<p>Price: $@Model["order"].Price</p>
<p>Date: @Model["order"].Date</p>
<p>Vendor: @Model["vendor"].Name</p>

<a href='/vendors/@Model["vendor"].Id'><button>Back to vendor</button></a>